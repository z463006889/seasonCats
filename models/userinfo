CREATE TABLE `NewTable` (
`userinfo_id`  int NOT NULL AUTO_INCREMENT ,
`userinfo_user_id`  int NOT NULL COMMENT '用户id' ,
`userinfo_user_niceName`  varchar(64) CHARACTER SET utf8 COLLATE utf8_general_ci NULL COMMENT '用户昵称' ,
`userinfo_phone`  varchar(16) CHARACTER SET utf8 COLLATE utf8_general_ci NULL COMMENT '用户手机号码' ,
`userinfo_cardId`  varchar(20) CHARACTER SET utf8 COLLATE utf8_general_ci NULL COMMENT '用户身份证号码' ,
`userinfo_address`  varchar(64) CHARACTER SET utf8 COLLATE utf8_general_ci NULL COMMENT '用户地址' ,
PRIMARY KEY (`userinfo_id`)
)
;